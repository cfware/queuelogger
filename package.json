{
	"name": "@aqm/queuelogger",
	"version": "1.1.0",
	"description": "CFWare AQM queue_log mysql writer",
	"main": "index.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc mocha"
	},
	"engines": {
		"nodejs": ">=8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/queuelogger.git"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"keywords": [
		"asterisk",
		"queue_log",
		"queuemetrics",
		"mysql"
	],
	"files": [
		"*.js",
		"sampledb",
		"test"
	],
	"bugs": {
		"url": "https://github.com/cfware/queuelogger/issues"
	},
	"homepage": "https://github.com/cfware/queuelogger#readme",
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"dbhost": "localhost",
		"dbprivuser": "root",
		"dbprivpassword": ""
	},
	"dependencies": {
		"@cfware/p-mutex": "^0.1.0",
		"mysql2": "^1.6.5",
		"p-event": "^4.0.0"
	},
	"devDependencies": {
		"mocha": "^6.0.2",
		"nyc": "^14.0.0",
		"standard-version": "^6.0.1",
		"xo": "^0.24.0"
	},
	"xo": {
		"rules": {
			"camelcase": [
				"error",
				{
					"properties": "never"
				}
			],
			"max-params": "off"
		}
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"index.js"
		]
	}
}
