{
	"name": "@aqm/queuelogger",
	"version": "2.0.0",
	"description": "CFWare AQM queue_log mysql writer",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava"
	},
	"engines": {
		"nodejs": ">=12"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/queuelogger.git"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"keywords": [
		"asterisk",
		"queue_log",
		"queuemetrics",
		"mysql"
	],
	"bugs": {
		"url": "https://github.com/cfware/queuelogger/issues"
	},
	"homepage": "https://github.com/cfware/queuelogger#readme",
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"dbhost": "localhost",
		"dbprivuser": "root",
		"dbprivpassword": ""
	},
	"dependencies": {
		"@cfware/p-mutex": "^0.2.0",
		"mysql2": "^1.6.5",
		"p-event": "^4.1.0"
	},
	"devDependencies": {
		"@cfware/eslint-config-node": "^0.1.0",
		"@cfware/nyc": "^0.5.0",
		"ava": "^2.1.0",
		"nyc": "^14.1.1",
		"standard-version": "^7.0.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"extends": "@cfware/eslint-config-node",
		"rules": {
			"camelcase": [
				"error",
				{
					"properties": "never"
				}
			],
			"max-params": "off"
		}
	}
}
