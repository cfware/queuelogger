{
	"name": "@cfware/queue_log-mysql",
	"version": "0.2.0",
	"description": "CFWare queue_log mysql writer",
	"main": "index.js",
	"scripts": {
		"test": "xo && nyc mocha"
	},
	"engines": {
		"nodejs": ">=8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/queue_log-mysql.git"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"keywords": [
		"asterisk",
		"queue_log",
		"queuemetrics",
		"mysql"
	],
	"files": [
		"*.js",
		"sampledb",
		"test"
	],
	"bugs": {
		"url": "https://github.com/cfware/queue_log-mysql/issues"
	},
	"homepage": "https://github.com/cfware/queue_log-mysql#readme",
	"config": {
		"dbhost": "localhost",
		"dbprivuser": "root",
		"dbprivpassword": ""
	},
	"dependencies": {
		"@cfware/p-mutex": "^0.1.0",
		"mysql": "^2.15.0",
		"p-event": "^1.3.0"
	},
	"devDependencies": {
		"eslint": "^4.12.1",
		"mocha": "^4.0.1",
		"nyc": "^11.4.0",
		"xo": "^0.20.0"
	},
	"xo": {
		"rules": {
			"comma-dangle": [
				"error",
				"always-multiline"
			],
			"camelcase": "off",
			"max-params": "off"
		}
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"index.js"
		]
	}
}
